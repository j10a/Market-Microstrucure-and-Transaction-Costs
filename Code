    "Y_buy= mktcap['Buy_Transaction_Cost'].astype(float)\n",
    "X_buy = mktcap['log_mktcap'].astype(float)\n",
    "X_buy = sm.add_constant(X_buy)\n",
    "model_buy = sm.OLS(Y_buy, X_buy).fit(cov_type='HC1')\n",
